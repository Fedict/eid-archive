[beid-release]
name=Belgian eID package archive
baseurl=http://files.eid.belgium.be/rpm/opensuse/$releasever
enabled=1
gpgcheck=1
autorefresh=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-BEID-RELEASE

[beid-continuous]
name=Belgian eID package archive - unsupported prerelease builds
baseurl=http://files.eid.belgium.be/rpm/continuous/opensuse/$releasever
enabled=0
gpgcheck=1
autorefresh=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-BEID-CONTINUOUS
